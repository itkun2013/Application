            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            >
            <com.konsung.defineview.ImageTextButton
                android:id="@+id/check_health_btn"
                android:layout_width="200dp"
