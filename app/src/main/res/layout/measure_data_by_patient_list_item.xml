                android:layout_below="@id/measure_time"
                android:layout_marginRight="20dp"
                android:layout_marginTop="5dp"
                android:orientation="horizontal"
                android:visibility="gone">
                <!--respRr-->
                <TextView
                    android:id="@+id/resp_rr_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/resp_rr"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/resp_rr"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:textSize="16sp" />
            </LinearLayout>

            <!--spo2参数-->
            <LinearLayout
                android:id="@+id/spo2_ll"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:orientation="horizontal">
                <!--spo2Trend-->
                <TextView
                    android:id="@+id/spo2_trend_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/spo2_trend"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/spo2_trend"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:textSize="16sp" />
                <!--spo2Pr-->
                <TextView
                    android:id="@+id/spo2_pr_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:text="@string/spo2_pr"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/spo2_pr"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:textSize="16sp" />
            </LinearLayout>

            <!--temp参数-->
            <LinearLayout
                android:id="@+id/temp_ll"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignWithParentIfMissing="true"
                android:layout_below="@id/measure_time"
                android:layout_marginTop="5dp"
                android:layout_toRightOf="@id/spo2_ll"
                android:orientation="horizontal"
                android:visibility="gone">
                <!--t1-->
                <TextView
                    android:id="@+id/temp_t1_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/temp_t1"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/temp_t1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:textSize="16sp" />
                <!--t2-->
                <TextView
                    android:id="@+id/temp_t2_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:text="@string/temp_t2"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/temp_t2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:textSize="16sp" />
                <!--td-->
                <TextView
                    android:id="@+id/temp_td_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:text="@string/temp_td"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/temp_td"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:textSize="16sp" />
            </LinearLayout>

            <!--nibp参数-->
            <LinearLayout
                android:id="@+id/nibp_ll"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignWithParentIfMissing="true"
                android:layout_below="@id/measure_time"
                android:layout_marginTop="5dp"
                android:layout_toRightOf="@id/temp_ll"
                android:orientation="horizontal">
                <!--nibp-->
                <TextView
                    android:id="@+id/nibp_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/nibp"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/nibp_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:textSize="16sp" />
            </LinearLayout>

            <!--temp参数-->
            <LinearLayout
                android:id="@+id/temp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignWithParentIfMissing="true"
                android:layout_below="@id/measure_time"
                android:layout_marginTop="5dp"
                android:layout_toRightOf="@id/temp_ll"
                android:orientation="horizontal">
                <!--nibp-->
                <TextView
                    android:id="@+id/temp_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/irtemp_trend"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/temp_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:textSize="16sp" />
            </LinearLayout>
            <!--bloodglu参数-->
            <LinearLayout
                android:id="@+id/blood_glu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:orientation="horizontal">
                <!--nibp-->
                <TextView
                    android:id="@+id/blood_glu_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/blood_glu_trend"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/blood_glu_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:textSize="16sp" />
            </LinearLayout>
            <!--bloodwbc参数-->
            <LinearLayout
                android:id="@+id/blood_wbc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/blood_wbc_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/blood_wbc_trend"
                    android:textSize="16sp" />
                <TextView
                    android:id="@+id/blood_wbc_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:textSize="16sp" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical"
            >
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
            <!--<TextView-->
                <!--android:layout_width="wrap_content"-->
                <!--android:layout_height="wrap_content"-->
                <!--android:text="@string/urinert"-->
                <!--/>-->
            </LinearLayout>


            <LinearLayout
